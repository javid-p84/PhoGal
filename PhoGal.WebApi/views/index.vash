@html.extend('layout',function(mdoel){
	@html.block("body",function(model){


<form action="/v3" method="post">
    <input name="name" type="text" />
    <input type="submit" />
    <p style="color:red">
        @if(model.msg)
        @model.msg
    </p>
</form>


<h1>
    @model.hey

</h1>

    @model.others.files.forEach(function(file){
        <p>file: @file</p> 
        <br />
    })

    @model.others.albums.forEach(function(album){
        <p>dir: @album</p>
        <br />
    })

	})
})